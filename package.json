{
  "name": "graphql-utilities",
  "version": "0.0.14",
  "description": "Utilities for GraphQL",
  "author": "Miguel Oller <miguel@bloveit.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bloveit/graphql-utilities.git"
  },
  "bugs": "https://github.com/bloveit/graphql-utilities/issues",
  "keywords": [
    "GraphQL"
  ],
  "main": "lib",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "build": "babel src -d lib --ignore __tests__",
    "prepublish": "in-publish && npm run lint && flow check && npm run test && npm run build || not-in-publish"
  },
  "peerDependencies": {
    "graphql": "^0.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-env": "^1.1.8",
    "coveralls": "^2.11.12",
    "eslint": "^3.11.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.38.0",
    "flow-typed": "^2.0.0",
    "graphql": "^0.9.1",
    "in-publish": "^2.0.0",
    "jest": "^18.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/**/__tests__/**"
    ],
    "testEnvironment": "node",
    "testRegex": "__tests__/.*-test.js$"
  }
}
